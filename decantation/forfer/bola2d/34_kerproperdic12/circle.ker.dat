  PHYSICAL_PROBLEM
    PROPERTIES: 
        MATERIAL= 1
           DENSITY:       BIFLUID,  PARAMETERS= 100.0,  1.0
           VISCOSITY:     BIFLUID,  PARAMETERS= 0.5e-0, 0.5e-2
        END_MATERIAL
    END_PROPERTIES: 
    THICKNESS:            0.25
    COUPLING
      NASTIN ALEFOR  1
      LEVELS ALEFOR  1
    END_COUPLING
    GRAVITY:         On, NORM=30.0e-3,GX=0.0,GY=-1.0,GZ=0.0
    DENSITY=         1.0
    VISCOSITY=       1.0
    WALL_LAW:        WALLD=0.01
  END_PHYSICAL_PROBLEM
  NUMERICAL_TREATMENT
     MESH
     END_MESH
     WALL_DISTANCE
       ALGEBRAIC_SOLVER:        deflated_CG, ITERA= 1000,TOLER= 1e-8
       PRECONDITIONING:         diagonal
       CODES BOUNDARIES
          5      1
       END_CODES
     END_WALL_DISTANCE
  END_NUMERICAL_TREATMENT
  OUTPUT_&_POST_PROCESS 
    ON_LAST_MESH
    STEPS= 200
    POSTPROCESS DENSI,        STEPS = 1
  END_OUTPUT_&_POST_PROCESS  
